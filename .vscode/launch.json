    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Project",
            "type": "debugpy",
            "request": "launch",
            "module": "songdo_traffic",
            "console": "integratedTerminal"
        },
        {
            "name": "Generate METR-IMC Dataset",
            "type": "debugpy",
            "request": "launch",
            "module": "songdo_traffic.dataset.metr_imc",
            "console": "integratedTerminal",
        },
        {
            "name": "Collect Incheon Traffic Data",
            "type": "debugpy",
            "request": "launch",
            "module": "songdo_traffic.dataset.imcrts",
            "console": "integratedTerminal",
            "args": ["--date_range", "20231225-20231231"]
        },
        {
            "name": "Generate Incheon Node-Link",
            "type": "debugpy",
            "request": "launch",
            "module": "songdo_traffic.dataset.nodelink",
            "console": "integratedTerminal",
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}